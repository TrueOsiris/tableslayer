{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "web#migrate": {
      "outputs": [],
      "env": ["TURSO_APP_DB_URL", "TURSO_APP_DB_AUTH_TOKEN"]
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT", ".env*"],
      "outputs": [".svelte-kit/**"]
    },
    "web#build": {
      "dependsOn": ["^web#migrate"],
      "env": [
        "ENV_NAME",
        "GITHUB_PR_NUMBER",
        "TURSO_API_TOKEN",
        "TURSO_APP_DB_URL",
        "TURSO_APP_DB_AUTH_TOKEN",
        "RESEND_TOKEN",
        "RESEND_AUDIENCE_ID",
        "DEV_EMAIL",
        "BASE_URL",
        "CLOUDFLARE_ACCOUNT_ID",
        "CLOUDFLARE_R2_ACCESS_KEY",
        "CLOUDFLARE_R2_SECRET_KEY",
        "CLOUDFLARE_R2_BUCKET_NAME",
        "SENTRY_DSN",
        "SENTRY_AUTH_TOKEN",
        "BODY_SIZE_LIMIT",
        "STRIPE_SECRET_KEY",
        "STRIPE_WEBHOOK_KEY",
        "STRIPE_LIFETIME_ID",
        "STRIPE_YEARLY_ID",
        "STRIPE_PRICE_ID_MONTHLY",
        "STRIPE_PRICE_ID_YEARLY",
        "STRIPE_PRICE_ID_LIFETIME",
        "STRIPE_WEBHOOK_KEY",
        "STRIPE_API_KEY"
      ],
      "outputs": [".svelte-kit/**"]
    },
    "web#start": {
      "env": [
        "ENV_NAME",
        "GITHUB_PR_NUMBER",
        "TURSO_API_TOKEN",
        "TURSO_APP_DB_URL",
        "TURSO_APP_DB_AUTH_TOKEN",
        "RESEND_TOKEN",
        "RESEND_AUDIENCE_ID",
        "DEV_EMAIL",
        "BASE_URL",
        "CLOUDFLARE_ACCOUNT_ID",
        "CLOUDFLARE_R2_ACCESS_KEY",
        "CLOUDFLARE_R2_SECRET_KEY",
        "CLOUDFLARE_R2_BUCKET_NAME",
        "SENTRY_DSN",
        "SENTRY_AUTH_TOKEN",
        "BODY_SIZE_LIMIT",
        "STRIPE_SECRET_KEY",
        "STRIPE_WEBHOOK_KEY",
        "STRIPE_LIFETIME_ID",
        "STRIPE_YEARLY_ID",
        "STRIPE_PRICE_ID_MONTHLY",
        "STRIPE_PRICE_ID_YEARLY",
        "STRIPE_PRICE_ID_LIFETIME",
        "STRIPE_WEBHOOK_KEY",
        "STRIPE_API_KEY"
      ],
      "persistent": true,
      "cache": false
    },
    "docs#start": {
      "env": ["BASE_URL"],
      "persistent": true,
      "cache": false
    },
    "docs#build": {
      "env": ["BASE_URL"],
      "outputs": [".svelte-kit/**"]
    },
    "tsc": {
      "dependsOn": ["^tsc"],
      "outputs": []
    },
    "check": {
      "dependsOn": ["^check"],
      "outputs": []
    },
    "lint": {},
    "test": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:party": {
      "cache": false,
      "persistent": true
    }
  }
}
